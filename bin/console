#!/usr/local/bin/php
<?php

declare(strict_types=1);

use App\Company\CompanyBuilder;
use App\Company\Crawler\CompanyFullNameCrawler;
use App\Company\Crawler\EstimateReturnCrawler;
use App\Company\ReadModel\TickerSymbol;
use App\FinYahoo;
use Symfony\Component\HttpClient\HttpClient;

require dirname(__DIR__) . '/vendor/autoload.php';

$financeYahoo = new FinYahoo(
    HttpClient::create(),
    new CompanyBuilder([
        'CompanyFullName' => new CompanyFullNameCrawler(),
        'EstimateReturn' => new EstimateReturnCrawler(),
    ])
);

$companies = $financeYahoo->crawlStock(
    new TickerSymbol('BABA'),
    new TickerSymbol('AAPL'),
);

dump($companies);
